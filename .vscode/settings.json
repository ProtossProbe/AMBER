{
    "code-runner.executorMap": {
        "cpp": "cd $dir && clang++ -std=c++14 -Wall -Wno-missing-braces -Wno-unused-variable -Wno-unused-parameter -Wextra -pthread $fileName -o  ../bin/$fileNameWithoutExt && ../bin/$fileNameWithoutExt"
    },
    "files.associations": {
        "*.dat": "fortran-modern",
        "*.in": "fortran-modern",
        "*.m": "matlab",
        "*.vtk": "fortran-modern",
        "*.ipp": "cpp",
        "cmath": "cpp",
        "complex": "cpp",
        "ostream": "cpp",
        "iosfwd": "cpp",
        "iomanip": "cpp",
        "fstream": "cpp",
        "locale": "cpp",
        "*.tcc": "cpp",
        "iostream": "cpp",
        "bitset": "cpp",
        "iterator": "cpp",
        "__locale": "cpp",
        "functional": "cpp",
        "cstdlib": "cpp",
        "valarray": "cpp",
        "__config": "cpp",
        "cstddef": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "new": "cpp",
        "stdexcept": "cpp",
        "typeinfo": "cpp",
        "algorithm": "cpp",
        "vector": "cpp",
        "string": "cpp",
        "ratio": "cpp",
        "array": "cpp",
        "queue": "cpp",
        "chrono": "cpp",
        "limits": "cpp",
